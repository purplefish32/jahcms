<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor>
  <parser>
      <default-package-name>phpDocumentor</default-package-name>
      <target>app/doc/</target>
  </parser>
  <transformer>
    <target>app/doc/</target>
  </transformer>
  <files>
    <directory>src/*</directory>
    <ignore>*Tests*</ignore>
    <ignore>*vendor*</ignore>
  </files>
</phpdocumentor>
